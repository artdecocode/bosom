{
  "name": "bosom",
  "version": "1.0.0",
  "description": "Read and write a JSON file in Node.js",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "compile": "depack-dev src/depack -o compile/bosom.js -a -c -p -s --source_map_include_content -O 2018",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "lint": "eslint .",
    "e": "alanode",
    "example/read.js": "yarn e example/read.js",
    "example/write.js": "yarn e example/write.js",
    "externs": "typal types/externs.js",
    "d1": "typal compile/index.js -c",
    "d": "yarn-s d1 externs"
  },
  "files": [
    "compile",
    "src",
    "externs.js"
  ],
  "externs": "externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/bosom.git"
  },
  "keywords": [
    "bosom",
    "json",
    "read",
    "write"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/bosom/issues"
  },
  "homepage": "https://github.com/artdecocode/bosom#readme",
  "devDependencies": {
    "alamode": "^2.3.6",
    "documentary": "^1.29.0",
    "eslint-config-artdeco": "^1.0.1",
    "temp-context": "^2.1.3",
    "typal": "^1.19.0",
    "yarn-s": "^1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "@wrote/read": "^1.0.4",
    "@wrote/write": "^1.0.4"
  }
}
